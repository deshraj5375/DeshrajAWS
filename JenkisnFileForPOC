import groovy.json.JsonSlurper


node
{
 def result
 
    stage('checkout')
    {
        git changelog: false, poll: false, url: 'https://github.com/deshraj5375/DeshrajAws.git'
    }
    
    stage('reading json file')
	{
	  def output =sh label: '', returnStdout: true, script: ' cat docker-test.json'  
	  def slurper = new JsonSlurper()
      def output1 =  slurper.parseText(output)
      result =output1.docker_pull
	  echo "${result}"
	 
        
    }
	stage('json file')
	{ 
	    echo "test"
        echo "result is -> ${result}"    
    }
}
